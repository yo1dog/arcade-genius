{
  "defaultSeverity": "error",
  "jsRules": true,
  "rules": {
    "adjacent-overload-signatures": true,
    "ban-types": false,
    "member-access": true,
    "member-ordering": false,
    "no-any": true,
    "no-empty-interface": false,
    "no-import-side-effect": [true, {"ignore-module": "\\.(css|less)$"}],
    "no-inferrable-types": false,
    "no-internal-module": true,
    "no-magic-numbers": false,
    "no-namespace": true,
    "no-non-null-assertion": false,
    "no-parameter-reassignment": false,
    "no-reference": true,
    "no-unnecessary-type-assertion": true,
    "no-var-requires": true,
    "only-arrow-functions": false,
    "prefer-for-of": false,
    "promise-function-async": true,
    "typedef": true,
    "unified-signatures": true,
    
    "await-promise": true,
    "ban-comma-operator": true,
    "ban": false,
    "curly": [true, "ignore-same-line"],
    "forin": false,
    "function-constructor": true,
    "import-blacklist": false,
    "label-position": true,
    "no-arg": true,
    "no-async-without-await": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": false,
    "no-construct": true,
    "no-debugger": false,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": true,
    "no-dynamic-delete": false,
    "no-empty": true,
    "no-eval": true,
    "no-floating-promises": true,
    "no-for-in-array": true,
    "no-implicit-dependencies": [true, "dev"],
    "no-inferred-empty-object-type": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-misused-new": true,
    "no-null-keyword": false,
    "no-null-undefined-union": false,
    "no-object-literal-type-assertion": true,
    "no-return-await": false,
    "no-shadowed-variable": false,
    "no-sparse-arrays": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-submodule-imports": false,
    "no-switch-case-fall-through": false,
    "no-tautology-expression": true,
    "no-this-assignment": true,
    "no-unbound-method": true,
    "no-unnecessary-class": false,
    "no-unsafe-any": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "no-void-expression": [true, "ignore-arrow-function-shorthand"],
    "prefer-conditional-expression": false,
    "prefer-object-spread": true,
    "radix": true,
    "restrict-plus-operands": true,
    "static-this": true,
    "strict-boolean-expressions": false,
    "strict-comparisons": [true, {"allow-object-equal-comparison": true}],
    "strict-type-predicates": true,
    "switch-default": false,
    "triple-equals": true,
    "unnecessary-constructor": false,
    "use-default-type-parameter": false,
    "use-isnan": true,
    
    "cyclomatic-complexity": false,
    "deprecation": true,
    "max-classes-per-file": false,
    "max-file-line-count": false,
    "no-default-export": false,
    "no-duplicate-imports": false,
    "no-mergeable-namespace": true,
    "no-require-imports": true,
    "object-literal-sort-keys": false,
    "prefer-const": true,
    "prefer-readonly": false,
    
    "array-type":{"options": "array-simple"},
    "arrow-return-shorthand": false,
    "binary-expression-operand-order": false,
    "callable-types": false,
    "class-name": true,
    "comment-format": false,
    "comment-type": false,
    "completed-docs": false,
    "encoding": true,
    "file-header": false,
    "file-name-casing": [true, "camel-case"],
    "increment-decrement": false,
    "interface-name": true,
    "interface-over-type-literal": false,
    "match-default-export-name": false,
    "newline-per-chained-call": false,
    "no-angle-bracket-type-assertion": true,
    "no-boolean-literal-compare": true,
    "no-parameter-properties": true,
    "no-redundant-jsdoc": true,
    "no-reference-import": true,
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-initializer": false,
    "no-unnecessary-qualifier": false,
    "object-literal-key-quotes": false,
    "object-literal-shorthand": false,
    "one-line": false,
    "one-variable-per-declaration": false,
    "ordered-imports": false,
    "prefer-function-over-method": false,
    "prefer-method-signature": true,
    "prefer-switch": false,
    "prefer-template": false,
    "prefer-while": false,
    "return-undefined": false,
    "space-before-function-paren": false,
    "space-within-parens": false,
    "switch-final-break": false,
    "type-literal-delimiter": true,
    "unnecessary-bind": true,
    "unnecessary-else": true,
    "variable-name": {"options": ["check-format", "allow-leading-underscore", "ban-keywords"]},
    
    "align": true,
    "arrow-parens": false,
    "eofline": false,
    "import-spacing": false,
    "indent": false,
    "jsdoc-format": true,
    "linebreak-style": [true, "LF"],
    "max-line-length": false,
    "newline-before-return": false,
    "new-parens": true,
    "no-consecutive-blank-lines": false,
    "no-irregular-whitespace": false,
    "no-trailing-whitespace": false,
    "number-literal-format": true,
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": true,
    "trailing-comma": false,
    "typedef-whitespace": false,
    "whitespace": false
  }
}